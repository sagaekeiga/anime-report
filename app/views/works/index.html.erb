<%= link_to "新規登録", new_work_path %>



<div class="col-xs-12">
<table class="type07">
    <thead>
    <tr>
        <th scope="cols"></th>
        <th scope="cols">メインタイトル</th>
        <th scope="cols">サブタイトル</th>
        <th scope="cols">内容</th>
        <th scope="cols">Youtube</th>
        <th scope="cols">放映日</th>
        <th scope="cols">管理</th>
    </tr>
    </thead>
    <tbody>
        
    <% @works.each do |work| %>

        <tr>
            <th scope="row"><%= work.id %></th>
    		<td><%= work.main_title %></td>
    		<td><%= work.sub_title %></td>
    		<td><%= work.content %></td>
    		<td><%= link_to "#{work.youtube}", work.youtube %></td>
    		<td><%= work.date %></td>
    		<td>
    		    <%= link_to "編集", edit_work_path(work.id) %>
    		    <%= link_to "削除", work, method: :delete, data: { confirm: "本当に削除してもいいですか？" } %>
    		    
    		</td>
        </tr>
        
    <% end %>
    </tbody>
</table>
</div>


